{
  "name": "community-code",
  "type": "module",
  "scripts": {
    "build": "nuxt generate",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "lint:spelling": "cspell **/*.md --config=./cspell.json",
    "lint:markdown": "markdownlint-cli2 \"content/**/*.md\" --config \".markdownlint.json\"",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix:prettier": "prettier --write .",
    "test:github": "playwright test",
    "test:local": "playwright test --headed",
    "prepare": "node .husky/install.mjs",
    "postinstall": "nuxt prepare",
    "ci:check": "bun run lint:eslint && bun run lint:prettier && bun run lint:spelling && bun run lint:markdown",
    "release": "semantic-release"
  },
  "dependencies": {
    "@matterlabs/docs-nuxt-template": "2.7.0",
    "@nuxt/content": "^2.13.2",
    "@nuxt/eslint": "^0.3.13",
    "@nuxt/fonts": "^0.5.1",
    "@nuxt/image": "^1.7.0",
    "@nuxt/ui": "^2.18.4",
    "@nuxt/ui-pro": "^1.4.1",
    "@nuxtjs/seo": "^2.0.0-rc.10",
    "dayjs": "^1.11.10",
    "nuxt": "^3.12.4",
    "nuxt-gtag": "^2.0.6",
    "nuxt-headlessui": "^1.2.0",
    "nuxt-og-image": "^3.0.0-rc.53",
    "rehype-katex": "^7.0.0",
    "remark-math": "^6.0.0",
    "vue-tsc": "^2.0.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@playwright/test": "^1.45.2",
    "@vue/test-utils": "^2.4.5",
    "cspell": "^8.7.0",
    "eslint": "^8.57.0",
    "ethers": "^6.13.1",
    "hardhat": "^2.22.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "markdownlint": "^0.34.0",
    "markdownlint-cli2": "^0.13.0",
    "node-pty": "^1.0.0",
    "pm2": "^5.4.2",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "zksync-ethers": "^6.11.0"
  }
}
